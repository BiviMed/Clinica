@model ClinicaBase.Responses.GeneralResponse

@{
    ViewData["Title"] = "Error";
}

<div class="container">
    <div class="text-center">
        <h1>Error</h1>
    </div>

    @{
        if (Model != null)
        {
            <div class="text-center">
                <label>@Model.Message <a asp-controller="Home" asp-action="Index">Ir a Home</a></label>
            </div>                        
        }
    }
</div>

